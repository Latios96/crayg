#usda 1.0
(
    subLayers = [
        @./../_base_scene/_base_scene.usda@
    ]
)

over "render_cam"
{
    over "render_camShape"{
        custom string craygCameraType = "REALISTIC"
        custom asset craygLensFile = @./Panavision.fx@
        float fStop = 1
    }
}

over "Render"
{
    over "settings"
    {
        uniform int2 resolution = (240, 135)
        custom int maxSamples = 8
    }
}